<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/tic"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivityTC" >

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="46dp"
        android:src="@drawable/ticback" />

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageView1"
        android:layout_alignParentBottom="true"
        android:text="Reset" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignLeft="@+id/imageView1"
        android:layout_alignTop="@+id/imageView1"
        android:layout_marginLeft="26dp" />

    <ImageView
        android:id="@+id/ImageView01"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignTop="@+id/imageView1"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/ImageView02"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignRight="@+id/imageView1"
        android:layout_alignTop="@+id/imageView1"
        android:layout_marginRight="24dp" />

    <ImageView
        android:id="@+id/ImageView03"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignRight="@+id/button1"
        android:layout_below="@+id/imageView2"
        android:layout_marginTop="22dp" />

    <ImageView
        android:id="@+id/ImageView04"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignLeft="@+id/ImageView01"
        android:layout_alignTop="@+id/ImageView03" />

    <ImageView
        android:id="@+id/ImageView05"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignLeft="@+id/ImageView02"
        android:layout_alignTop="@+id/ImageView04" />

    <ImageView
        android:id="@+id/ImageView06"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_above="@+id/button1"
        android:layout_alignRight="@+id/button1" />

    <ImageView
        android:id="@+id/ImageView07"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignLeft="@+id/ImageView04"
        android:layout_alignTop="@+id/ImageView06" />

    <ImageView
        android:id="@+id/ImageView08"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignLeft="@+id/ImageView05"
        android:layout_alignTop="@+id/ImageView07" />

    <ImageView
        android:id="@+id/line_h1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView2"
        android:layout_alignRight="@+id/ImageView02" />

    <ImageView
        android:id="@+id/line_h2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/ImageView03"
        android:layout_alignTop="@+id/ImageView03"
        android:layout_marginTop="14dp" />

    <ImageView
        android:id="@+id/line_h3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/ImageView06"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/line_v1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/ImageView06"
        android:layout_alignRight="@+id/ImageView03" />

    <ImageView
        android:id="@+id/line_v2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView1"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/line_v3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/ImageView08"
        android:layout_alignLeft="@+id/ImageView02" />

    <ImageView
        android:id="@+id/diagonal_357"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView1"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/diagonal_159"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/ImageView06"
        android:layout_alignLeft="@+id/imageView2" />

</RelativeLayout>